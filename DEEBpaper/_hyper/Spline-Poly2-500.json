{
  "_class": ["Trajs", "HyperParms", "Opts"],
  "name": "Spline-Poly2",
  "normalize": "meanAndCov",
  "initialState": {
    "_class": ["FromObs", "InitialState", "Opts"]
  },
  "fitTrajs": {
    "_class": ["InterpolationSpline", "FitTrajs", "Opts"],
    "target": "point",
    "interSteps": 10
  },
  "derivMethod": "none",
  "derivFun": {
    "_class": ["GlobalLm", "DerivFun", "Opts"],
    "features": {
      "z": ["1", "x", "x1*x2", "x1*x3", "x2*x3", "x^2"]
    }
  },
  "odeSolver": {
    "_class": ["OdeSolver", "Opts"],
    "timeStep": 0.002
  }
}
